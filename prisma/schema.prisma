generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Enquete {
  codigo     String   @id @default(uuid())
  titulo     String
  descricao  String
  criadoEm   DateTime @default(now())
  alteradoEm DateTime @default(now())
}
